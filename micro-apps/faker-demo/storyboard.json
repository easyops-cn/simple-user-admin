{
  "$schema": "../../node_modules/@easyops/brick-types/.schema/storyboard.json",
  "imports": ["@bricks/basic-bricks"],
  "app": {
    "name": "Faker Demo",
    "id": "faker-demo",
    "homepage": "/faker-demo"
  },
  "routes": [
    {
      "path": "/faker-demo",
      "bricks": [
        {
          "brick": "basic-bricks.micro-view",
          "slots": {
            "content": {
              "type": "bricks",
              "bricks": [
                {
                  "brick": "div",
                  "properties": {
                    "textContent": "hello, faker"
                  }
                },
                {
                  "brick": "user-admin.user-admin"
                },
                {
                  "brick": "user-admin.provider-user-list",
                  "bg": true
                },
                {
                  "brick": "presentational-bricks.brick-table",
                  "lifeCycle": {
                    "useResolves": [
                      {
                        "provider": "user-admin\\.provider-user-list",
                        "name": "dataSource"
                      }
                    ]
                  },
                  "properties": {
                    "columns": [
                      {
                        "title": "名称",
                        "dataIndex": "name"
                      },
                      {
                        "title": "年龄",
                        "dataIndex": "age"
                      },
                      {
                        "title": "地址",
                        "dataIndex": "address"
                      }
                    ]
                  }
                }
              ]
            }
          }
        }
      ]
    }
  ]
}
