{
  "name": "simple-user-admin",
  "private": true,
  "license": "UNLICENSED",
  "homepage": "https://github.com/easyops-cn/simple-user-admin",
  "scripts": {
    "prepare": "lerna bootstrap",
    "start": "lerna run start",
    "build": "npm run build:libs && npm run build:bricks && npm run build:templates && npm run build:micro-apps",
    "build:libs": "if [ -d libs ]; then lerna run build --scope \"@libs/*\"; else echo \"no libs\"; fi",
    "build:bricks": "if [ -d bricks ]; then lerna run build --scope \"@bricks/*\"; else echo \"no bricks\"; fi",
    "build:templates": "if [ -d templates ]; then lerna run build --scope \"@templates/*\"; else echo \"no templates\"; fi",
    "build:micro-apps": "if [ -d micro-apps ]; then lerna run build --scope \"@micro-apps/*\"; else echo \"no micro-apps\"; fi",
    "postbuild": "npm run size-limit",
    "size-limit": "size-limit",
    "serve": "brick-container-serve",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest --ci",
    "yo": "brick-scripts",
    "yo-debug": "node --inspect-brk ./node_modules/@easyops/brick-scripts/dist/index.js",
    "renew": "dev-dependencies-renew",
    "extract": "dev-dependencies-extract"
  },
  "browserslist": {
    "development": [
      "extends @easyops/browserslist-config-next/development"
    ],
    "production": [
      "extends @easyops/browserslist-config-next/production"
    ]
  },
  "devDependencies": {
    "@bricks/basic-bricks": "^1.11.0",
    "@bricks/container-brick": "^2.10.3",
    "@bricks/forms": "^1.48.0",
    "@bricks/presentational-bricks": "^1.67.0",
    "@easyops/brick-http": "^1.0.1",
    "@easyops/brick-icons": "^1.16.2",
    "@easyops/brick-kit": "^1.34.1",
    "@easyops/brick-types": "^1.30.0",
    "@easyops/brick-utils": "^1.40.0",
    "@easyops/dev-dependencies": "^0.5.8",
    "@easyops/fontawesome-library": "^1.1.3",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@size-limit/file": "^4.0.2",
    "@types/classnames": "^2.2.9",
    "@types/d3": "^5.7.2",
    "@types/dompurify": "^2.0.1",
    "@types/echarts": "^4.4.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/lolex": "^5.1.0",
    "@types/node": "^12.12.29",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.2.4",
    "@types/yaml": "^1.2.0",
    "antd": "^3.26.14",
    "d3-drag": "^1.2.5",
    "d3-force": "^2.0.1",
    "d3-hierarchy": "^1.1.9",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-zoom": "^1.8.3",
    "echarts": "^4.7.0",
    "enzyme": "^3.11.0",
    "history": "^4.10.1",
    "i18next": "^19.3.3",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "lolex": "^5.1.2",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-ace": "^7.0.4",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-transition-group": "^4.3.0"
  },
  "workspaces": [
    "bricks/*",
    "libs/*",
    "micro-apps/*",
    "templates/*"
  ],
  "resolutions": {
    "@types/react": "^16.9.23",
    "lodash": "^4.17.15",
    "@easyops/brick-types": "^1.30.0"
  },
  "easyops": {
    "create-next-repo": "0.11.4",
    "dev-dependencies": "0.5.3"
  }
}
